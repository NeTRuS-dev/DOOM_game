{
  "compilerOptions": {
    "module": "ES6",
    "target": "ES6",
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "strict": true
  },
  "exclude": [
    "node_modules"
  ]
}